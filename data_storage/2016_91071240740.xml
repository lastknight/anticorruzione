        .ai { background-position: -850px 0 !important; }
        .html, .xhtml, .dhtml, .php, .asp, .css, .js, .inc { background-position: -900px 0 !important; }
        .dir { background-position: -950px 0 !important; }
        div.clear {
            clear:both;
